{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
     "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "paths": {
    "@colors*": ["./styles/colors.ts"],
    "@database": ["./database/database.ts"],
    "@models*": ["./database/models/*"],
     "@listas/*": ["./components/listas/*"],
     "@cards/*": ["./components/cards/*"],
     "@modais/*": ["./components/modais/*"],
     "@logo/*":["./assets/images/logo/*"],
     "@routeFunctions*": ["./functions/routesFunctions.ts"],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
      "types/**/*.d.ts"
, "components/listas/materias/duvidas/duvidaCard.tsx"  ]
}
