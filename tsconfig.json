{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
    "@colors*": ["./styles/colors.ts"],
     "@listas/*": ["./components/listas/*"],
     "@modais/*": ["./components/modais/*"],
     "@logo/*":["./assets/images/logo/*"],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
      "types/**/*.d.ts"
  ]
}
